  <entry>
    <form>
      <orth>Choquet</orth>
    </form>
    <gramGrp>
      <pos>s. m.</pos>
    </gramGrp>
    <def label="verbatim">
      <quote> </quote>
      <quote>1 - bûche. </quote>
